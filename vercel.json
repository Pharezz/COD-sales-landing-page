{
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-frame-options", "value": "ALLOWALL" },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://selar.co *.selar.co"
        }
      ]
    }
  ]
}
